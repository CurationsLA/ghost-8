{{!< default}}
{{#post}}
<article class="post eats-template framed-article {{post_class}}">
  <header class="post-header-frame">
    <h1 class="post-full-title">{{title}}</h1>
    <div class="post-full-meta">
      <time datetime="{{date published_at format="YYYY-MM-DD"}}">{{date published_at format="MMMM D, YYYY"}}</time>
      {{#if primary_tag}}<span class="latest-item-tag">{{primary_tag.name}}</span>{{/if}}
    </div>
  </header>
  <div class="post-body-frame">
    <div class="post-content">
      {{{content}}}
    </div>
    {{> "cta-subscribe"}}
  </div>
</article>
{{/post}}